# Geometry passed to DFTB+ via API

Hamiltonian = DFTB {
  SCC = Yes
  MaxSCCIterations = 5
  
  MaxAngularMomentum = {
    C = "p"
    H = "s"
  }
  
  Filling = Fermi {
    Temperature [Kelvin] = 300
    IndependentKFilling = No
  }

  SlaterKosterFiles = {
    H-H = "./H-H.skf"
    C-C = "./C-C.skf"
    C-H = "./C-H.skf"
  }
  
  KPointsAndWeights = SuperCellFolding {
1 0 0
0 1 0
0 0 1
0. 0. 0.
  }
  
  PolynomialRepulsive = {}
  ShellResolvedSCC = No
  OldSKInterpolation = No
  RangeSeparated = None {}
  ReadInitialCharges = No
  InitialCharges = {}
  SCCTolerance = 1.0000000000000001E-005
  Mixer = Broyden {
    MixingParameter = 0.20000000000000001
    InverseJacobiWeight = 1.0000000000000000E-002
    MinimalWeight = 1.0000000000000000
    MaximalWeight = 100000.00000000000
    WeightFactor = 1.0000000000000000E-002
  }
  
  EwaldParameter = 0.0000000000000000
  EwaldTolerance = 1.0000000000000001E-009
  HCorrection = None {}
  SpinPolarisation = {}
  ElectricField = {}
  Solver = RelativelyRobust {}
  Charge = 0.0000000000000000
  OnSiteCorrection = {}
  Dispersion = {}
  ThirdOrder = No
  ThirdOrderFull = No
  Differentiation = FiniteDiff {
    Delta = 1.2207031250000000E-004
  }
  ForceEvaluation = "Traditional"
}

Options = {
  WriteResultsTag = No
  WriteDetailedOut = No
  TimingVerbosity = 0
  WriteAutotestTag = Yes
  WriteDetailedXML = No
  RestartFrequency = 20
  RandomSeed = 0
  WriteHS = No
  WriteRealHS = No
  MinimiseMemoryUsage = No
  ShowFoldedCoords = No
  WriteChargesAsText = No
}

ParserOptions = {
  ParserVersion = 8
  WriteHSDInput = No
  WriteXMLInput = No
  StopAfterParsing = No
  IgnoreUnprocessedNodes = No
}

Analysis = {
  CalculateForces = Yes
  ProjectStates = {}
  WriteEigenvectors = No
  WriteBandOut = No
  MullikenAnalysis = Yes
  AtomResolvedEnergies = No
}

Driver = {}
ExcitedState = {}
Parallel = {
  UseOmpThreads = Yes
}
