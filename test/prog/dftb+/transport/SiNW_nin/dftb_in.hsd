Geometry = GenFormat {
<<< 'SiNW.gen'
}


Transport {
    Device {
      AtomRange = 1 600
    FirstLayerAtoms =  1   61   121   181   241   301   361   421   481   541 
    }
    Contact {
      Id = "source"
      AtomRange = 601 720
    FermiLevel [eV] = -2.4339
      potential = 0.0
    }
    Contact {
      Id = "drain"
      AtomRange = 721 840
    FermiLevel [eV] = -2.4339
      potential [eV] = 0.015
    }
  #Task = ContactHamiltonian{
  #   ContactId = "source"
  #}
}


Driver = {}

Hamiltonian = DFTB {
  SCC = Yes 
  SCCTolerance = 1e-6
  MaxSCCIterations = 200 
  MaxAngularMomentum = {
    Si ="p"
    H = "s"
    Sin = "p"
  }

  Filling = Fermi {
    Temperature [Kelvin] = 300
  }
  
  SlaterKosterFiles = Type2FileNames {
  Prefix = './'
  Separator = '-'
  Suffix = '.skf'
  }
	
  #KpointsAndWeights = SupercellFolding{
  #  8 0 0
  #  0 1 0
  #  0 0 1
  #  0.50 0.0 0.0
  #}

  Electrostatics = Poisson {
    Verbosity=91
    Poissonbox [Angstrom] = 50.0 50.0 50.0
    MinimalGrid [Angstrom] = 0.4 0.4 0.4
    PoissonAccuracy = 1e-6
    BoundaryRegion = Circle{}
    AtomDensityTolerance = 1e-6
  }

  Eigensolver = GreensFunction {
  }

}

Analysis{
  TunnelingAndDOS{
    EnergyRange [eV] = -3.0  -2.0
    EnergyStep [eV] = 0.01 
  }
}

options = {
  writeautotesttag = yes
}

parseroptions = {
  parserversion = 3
}

