Geometry = GenFormat {
<<< "geo_start.gen"
}

Driver = {}

Hamiltonian = DFTB {
  SCC = Yes
  SCCTolerance = 1e-8
  MaxSCCIterations = 100
  Mixer = Broyden {
    MixingParameter = 0.2
  }
  MaxAngularMomentum = {
    C = "p"
    O = "p"
  }
  KPointsandWeights = SupercellFolding{
  1 0 0
  0 1 0
  0 0 1
  0. 0. 0.
  }
  Charge = 0.0
  SpinPolarisation = {}
  Eigensolver = DivideAndConquer {}
  Filling = Fermi {
    Temperature [Kelvin] = 0.0
  }
  SlaterKosterFiles = Type2FileNames {
    Separator = "-"
    Suffix = ".skf"
  }

  ManyBodyDispersion = MBD{
    NOmegaGrid = 15
    Beta = 0.86
    KGrid = { 1 1 1 }
    Debug = Yes
  }

}

Options = {
  CalculateForces = Yes
  WriteEigenvectors = Yes
  WriteAutotestTag = Yes
}

ParserOptions = {
  ParserVersion = 3
}
