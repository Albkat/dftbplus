Geometry = GenFormat {
  <<< geo.gen
}

Hamiltonian = DFTB {
  SCC = Yes
  ReadInitialCharges = Yes
  MaxSCCIterations = 1
  MaxAngularMomentum = {
   H = "s"
   C = "p"
  }
  SlaterKosterFiles = {
    C-C = "C-C.skf"
    C-H = "C-H.skf"
    H-C = "H-C.skf"
    H-H = "H-H.skf"
  }
  RangeSep = {
    Algorithm = NB {}
  }
}

Options = {
  WriteAutotestTag = Yes
}

Analysis {
  CalculateForces = Yes
  AtomResolvedEnergies = Yes
}

ParserOptions = {
  ParserVersion = 7
}
